---
import "virtual:uno.css";
import "daisyui/theme/dark.css";
type Props = {
  class?: string;
};
const { class: classes }: Props = Astro.props;
---

<!doctype html>
<html lang="en" data-theme="dark">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title></title>
  </head>
  <body class={`${classes}`}>
    <slot />
  </body>
</html>
